"gallery": {
    "props": {
      "layouts": [
            {
            "name": "grid",
            "component": "GridSummary",
            "itemsPerRow": {
                "(min-width:1300px)": 2,
                "desktop": 2,
                "tablet": 2,
                "phone": 1
            }
            }
        ],
        "GridSummary": "product-summary.shelf"
    }
},

"search-content": {
    "blocks":[
        "gallery",
        "not-found"
    ]
},

"product-summary.shelf": {
    "children": [
        "product-summary-name",
        "product-summary-image",
        "product-summary-space",
        "condition-layout.product",
        "product-summary-price",
        "link.product"
    ]
},

"link.product": {
    "props": {
      "href": "/{slug}/p",
      "label": "Detalhes",
      "displayMode": "button"
    }
},

"condition-layout.product": {
    "children":[
        "condition.product#new"
    ]
},

"condition.product#new": {
    "props": {
        "conditions": [
        {
            "subject": "productId",
            "verb": "is",
            "object": "56"
        }
        ]
    },
    "children": [
        "rich-text#highlight"
    ]
},

"rich-text#highlight": {
    "props": {
        "text": "LANÃ‡AMENTO",
        "blockClass": "highlight",
        "textAlignment": "center",
        "textPosition": "center"
    }
}
